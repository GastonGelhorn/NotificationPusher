        "apns",
        "android",
        "gcm",
        "notification",
        "message",
        "push",
        "pusher"
    ],
    "homepage": "https://github.com/Ph3nol/NotificationPusher",
    "type": "standalone",
    "license": "MIT",
    "authors": [
        {
